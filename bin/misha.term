# Consular script to launch tabs for development

tab do
  run "cd ~/Web/mishakahn"
end

tab do
  run "cd ~/Web/mishakahn"
  run "sass --watch stylesheets:stylesheets --style compressed"
end

tab do
  run "cd ~/Web/mishakahn"
  run "coffee -w -c javascripts"
end

tab do
  run "cd ~/Web/mishakahn"
  run "vim ."
end
